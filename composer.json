{
    "name": "yproximite/wanna-speak-bundle",
    "description": "Access to WannaSpeak API",
    "keywords": ["statistics", "call", "tracking", "rank"],
    "homepage": "https://github.com/Yproximite/WannaSpeakBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Baptiste Blanchon",
            "email": "jean-baptiste@yproximite.com"
        },
        {
            "name": "Hugo Alliaume",
            "email": "hugo@yproximite.com"
        }
    ],
    "scripts": {
        "phpstan": "phpstan analyze --ansi",
        "php-cs-fixer": "php-cs-fixer fix -v",
        "php-cs-fixer@ci": "php-cs-fixer fix -v --diff --dry-run",
        "phpunit": "phpunit",
        "phpspec": "phpspec --ansi"
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/http-client": "^4.3 || ^5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^6.1",
        "phpunit/phpunit": "^8.5",
        "nyholm/symfony-bundle-test": "^1.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.10",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "phpstan/phpstan-phpunit": "^0.12.6"
    },
    "autoload": {
        "psr-4": {
            "Yproximite\\WannaSpeakBundle\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
