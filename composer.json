{
    "name": "yproximite/wanna-speak-bundle",
    "description": "Access to WannaSpeak API",
    "keywords": ["statistics", "call", "tracking", "rank"],
    "homepage": "https://github.com/Yproximite/WannaSpeakBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Baptiste Blanchon",
            "email": "jean-baptiste@yproximite.com"
        }
    ],
    "scripts": {
        "phpstan": "phpstan analyze --ansi",
        "php-cs-fixer": "php-cs-fixer fix -v",
        "php-cs-fixer@ci": "php-cs-fixer fix -v --diff --dry-run",
        "phpunit": "simple-phpunit"
    },
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^3.4 || ^4.0 || ^5.0",
        "php-http/httplug": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/message": "^1.2",
        "php-http/client-common": "^1.2",
        "php-http/multipart-stream-builder": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.52",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-strict-rules": "^0.12.5",
        "symfony/phpunit-bridge": "^3.4 || ^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": { "Yproximite\\WannaSpeakBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Yproximite\\WannaSpeakBundle\\Tests\\": "tests" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
