{
    "name": "yproximite/wanna-speak-bundle",
    "description": "Access to WannaSpeak API",
    "keywords": ["statistics", "call", "tracking", "rank"],
    "homepage": "https://github.com/Yproximite/WannaSpeakBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Baptiste Blanchon",
            "email": "jean-baptiste@yproximite.com"
        },
        {
            "name": "Hugo Alliaume",
            "email": "hugo@yproximite.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/framework-bundle": "^4.3",
        "symfony/http-client": "^4.3",
        "phpspec/phpspec": "^6.1"
    },
    "suggest": {
        "symfony/framework-bundle": "4.3",
        "symfony/http-client": "4.3"
    },
    "autoload": {
        "psr-4": { "Yproximite\\WannaSpeakBundle\\": "" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
