{
    "name": "yproximite/wanna-speak-bundle",
    "description": "Access to WannaSpeak API",
    "keywords": ["statistics", "call", "tracking", "rank"],
    "homepage": "https://github.com/Yproximite/WannaSpeakBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Baptiste Blanchon",
            "email": "jean-baptiste@yproximite.com"
        }
    ],
    "require": {
        "php": ">=5.4|>=7.0",
        "symfony/config": "^3.0|^4.0",
        "symfony/http-kernel": "^3.0|^4.0",
        "symfony/dependency-injection": "^3.0|^4.0",
        "php-http/httplug": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/message": "^1.2",
        "php-http/client-common": "^1.2"
    },
    "suggest": {
        "symfony/monolog-bundle": "^3.0"
    },
    "autoload": {
        "psr-4": { "Yproximite\\WannaSpeakBundle\\": "" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
