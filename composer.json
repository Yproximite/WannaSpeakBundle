{
    "name": "yproximite/wanna-speak-bundle",
    "description": "Access to WannaSpeak API",
    "keywords": ["statistics", "call", "tracking", "rank"],
    "homepage": "https://github.com/Yproximite/WannaSpeakBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Baptiste Blanchon",
            "email": "jean-baptiste@yproximite.com"
        },
        {
            "name": "Hugo Alliaume",
            "email": "hugo@yproximite.com"
        }
    ],
    "scripts": {
        "phpunit": "phpunit",
        "phpspec": "phpspec --ansi"
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/http-client": "^4.3 || ^5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^6.1",
        "phpunit/phpunit": "^8.5",
        "nyholm/symfony-bundle-test": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Yproximite\\WannaSpeakBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
